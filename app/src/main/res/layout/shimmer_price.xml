<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <LinearLayout
        android:id="@+id/cl_price_list_root"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/guideline_all_margin"
        android:layout_marginTop="@dimen/guideline_all_margin"
        android:layout_marginEnd="@dimen/guideline_all_margin"
        android:orientation="horizontal"
        tools:context=".ui.view.home.PriceListFragment">


        <com.google.android.material.card.MaterialCardView

            style="@style/Widget.5asec.CardView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_item_price_card"
            android:layout_marginStart="@dimen/margin_card"

            android:layout_weight="1">


        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView

            style="@style/Widget.5asec.CardView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_item_price_card"
            android:layout_marginStart="@dimen/margin_card"

            android:layout_weight="1">


        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView

            style="@style/Widget.5asec.CardView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/height_item_price_card"
            android:layout_marginStart="@dimen/margin_card"
            android:layout_weight="1">


        </com.google.android.material.card.MaterialCardView>

    </LinearLayout>
</layout>